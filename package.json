{
  "name": "linkedin-ai-assistant-login",
  "version": "1.0.0",
  "description": "Independent login/signup page for Requill",
  "main": "index.html",
  "scripts": {
    "generate-pricing": "node scripts/generate-pricing-config.js",
    "prebuild": "npm run generate-pricing",
    "dev": "npm run generate-pricing && next dev",
    "build": "next build",
    "export": "next build && next export",
    "start": "next start",
    "lint": "next lint",
    "serve": "npx http-server . -p 8080 -o",
    "preview": "npx http-server out -p 8080 -o",
    "deploy": "npm run build && firebase deploy --only hosting",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:all": "npm run build && firebase deploy"
  },
  "dependencies": {
    "firebase": "^11.10.0",
    "http-server": "^14.1.1",
    "lucide-react": "^0.525.0",
    "next": "^15.3.5",
    "razorpay": "^2.9.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0"
  },
  "keywords": [
    "react",
    "login",
    "signup",
    "authentication",
    "linkedin",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.1.0",
    "@types/node": "^24.0.13",
    "@types/react": "^19.1.8",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwind-scrollbar-hide": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.3"
  }
}
